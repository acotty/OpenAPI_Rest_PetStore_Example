{
  "source": "./src",
  "destination": "./dist/documentation",
  "unexportIdentifier": true,
  "undocumentIdentifier": true,
  "test": {
    "type": "mocha",
    "source": "./test"
  },
  "plugins": [
    {
      "name": "esdoc2-ecmascript-proposal-plugin",
      "option": {
        "classProperties": true,
        "objectRestSpread": true,
        "doExpressions": true,
        "functionBind": true,
        "functionSent": true,
        "asyncGenerators": true,
        "decorators": true,
        "exportExtensions": true,
        "dynamicImport": true
      }
    }
    // {
    //   "name": "esdoc2-standard-plugin"
    // },
    // {
    //   "name": "esdoc-typescript-plugin",
    //   "option": { "enable": false}
    // }
  ]
}


