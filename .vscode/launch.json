{
  // Use IntelliSense to learn about possible Node.js debug attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [  

    {
      "type": "node",
      "request": "launch",
      "name": "Debug data Service",
      "program": "${workspaceRoot}/index.js",
      "outFiles": ["${workspaceRoot}/dist/src/**/*.js"],
      "sourceMaps": true,
      "cwd": "${workspaceRoot}",
      "preLaunchTask":"Run Gulp on data service",
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome Model Swagger Data Service",
      "url": "http://gaming-service-model.local.max-test.com.au:10010/docs",
      "webRoot": ".",
      "sourceMaps": true,
      "trace": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Gulp tscompile task",
      "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
      "args": [
        "tscompile"
      ]
    }
  ]
}
